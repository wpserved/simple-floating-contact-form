{
  "name": "wps/simple-floating-contant-form",
  "type": "wordpress-plugin",
  "license": "GPL2",
  "description": "Simple Floating Contact Form",
  "homepage": "https://wpserved.com/",
  "authors": [
    {
      "name": "wpserved",
      "email": "dev@wpserved.com",
      "homepage": "https://wpserved.com/"
    }
  ],
  "keywords": [
    "wordpress"
  ],
  "autoload": {
    "psr-4": {
      "SimpleFloatingContactForm\\": "src/"
    },
    "files": [
      "inc/helpers.php"
    ]
  },
  "require": {
    "php": ">=7.2",
    "composer/installers": "~1.0",
    "illuminate/support": "7.4.0",
    "joachim-n/case-converter": "^1.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^2.8.0"
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true
    }
  }
}
